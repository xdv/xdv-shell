// Builtin help Tests

forge BuiltinHelpTests {
    
    proc test_k_run() -> UInt32 {
        let result = BuiltinHelp::K::run(0);
        return 1;
    }
    
    proc test_k_print_commands() -> UInt32 {
        let result = BuiltinHelp::K::print_commands();
        return 1;
    }
    
    proc test_k_print_usage() -> UInt32 {
        let result = BuiltinHelp::K::print_usage(0);
        return 1;
    }
    
    proc test_k_get_command_count() -> UInt32 {
        let result = BuiltinHelp::K::get_command_count();
        return 1;
    }
    
    proc test_q_run_returns_error() -> UInt32 {
        let result = BuiltinHelp::Q::run(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_print_commands_returns_error() -> UInt32 {
        let result = BuiltinHelp::Q::print_commands();
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_print_usage_returns_error() -> UInt32 {
        let result = BuiltinHelp::Q::print_usage(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_get_command_count() -> UInt32 {
        let result = BuiltinHelp::Q::get_command_count();
        return 1;
    }
    
    proc test_phi_run_returns_error() -> UInt32 {
        let result = BuiltinHelp::Phi::run(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_print_commands_returns_error() -> UInt32 {
        let result = BuiltinHelp::Phi::print_commands();
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_print_usage_returns_error() -> UInt32 {
        let result = BuiltinHelp::Phi::print_usage(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_get_command_count() -> UInt32 {
        let result = BuiltinHelp::Phi::get_command_count();
        return 1;
    }
    
    proc run_all_tests() -> UInt32 {
        let passed = 0;
        
        passed = passed + BuiltinHelpTests::test_k_run();
        passed = passed + BuiltinHelpTests::test_k_print_commands();
        passed = passed + BuiltinHelpTests::test_k_print_usage();
        passed = passed + BuiltinHelpTests::test_k_get_command_count();
        passed = passed + BuiltinHelpTests::test_q_run_returns_error();
        passed = passed + BuiltinHelpTests::test_q_print_commands_returns_error();
        passed = passed + BuiltinHelpTests::test_q_print_usage_returns_error();
        passed = passed + BuiltinHelpTests::test_q_get_command_count();
        passed = passed + BuiltinHelpTests::test_phi_run_returns_error();
        passed = passed + BuiltinHelpTests::test_phi_print_commands_returns_error();
        passed = passed + BuiltinHelpTests::test_phi_print_usage_returns_error();
        passed = passed + BuiltinHelpTests::test_phi_get_command_count();
        
        return passed;
    }
}
