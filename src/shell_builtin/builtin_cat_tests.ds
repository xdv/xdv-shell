// Builtin cat Tests

forge BuiltinCatTests {
    
    proc test_k_run() -> UInt32 {
        let result = BuiltinCat::K::run(0, 0);
        return 1;
    }
    
    proc test_k_read_file() -> UInt32 {
        let result = BuiltinCat::K::read_file(0);
        return 1;
    }
    
    proc test_k_print_content() -> UInt32 {
        let result = BuiltinCat::K::print_content(0, 0);
        return 1;
    }
    
    proc test_q_run_returns_error() -> UInt32 {
        let result = BuiltinCat::Q::run(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_read_file_returns_error() -> UInt32 {
        let result = BuiltinCat::Q::read_file(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_print_content_returns_error() -> UInt32 {
        let result = BuiltinCat::Q::print_content(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_run_returns_error() -> UInt32 {
        let result = BuiltinCat::Phi::run(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_read_file_returns_error() -> UInt32 {
        let result = BuiltinCat::Phi::read_file(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_print_content_returns_error() -> UInt32 {
        let result = BuiltinCat::Phi::print_content(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc run_all_tests() -> UInt32 {
        let passed = 0;
        
        passed = passed + BuiltinCatTests::test_k_run();
        passed = passed + BuiltinCatTests::test_k_read_file();
        passed = passed + BuiltinCatTests::test_k_print_content();
        passed = passed + BuiltinCatTests::test_q_run_returns_error();
        passed = passed + BuiltinCatTests::test_q_read_file_returns_error();
        passed = passed + BuiltinCatTests::test_q_print_content_returns_error();
        passed = passed + BuiltinCatTests::test_phi_run_returns_error();
        passed = passed + BuiltinCatTests::test_phi_read_file_returns_error();
        passed = passed + BuiltinCatTests::test_phi_print_content_returns_error();
        
        return passed;
    }
}
