// Builtin ls Tests

forge BuiltinLsTests {
    
    proc test_k_run() -> UInt32 {
        let result = BuiltinLs::K::run(0, 0);
        return 1;
    }
    
    proc test_k_list_entries() -> UInt32 {
        let result = BuiltinLs::K::list_entries(0);
        return 1;
    }
    
    proc test_k_print_entry() -> UInt32 {
        let result = BuiltinLs::K::print_entry(0, 0);
        return 1;
    }
    
    proc test_q_run_returns_error() -> UInt32 {
        let result = BuiltinLs::Q::run(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_list_entries_returns_error() -> UInt32 {
        let result = BuiltinLs::Q::list_entries(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_print_entry_returns_error() -> UInt32 {
        let result = BuiltinLs::Q::print_entry(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_run_returns_error() -> UInt32 {
        let result = BuiltinLs::Phi::run(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_list_entries_returns_error() -> UInt32 {
        let result = BuiltinLs::Phi::list_entries(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_print_entry_returns_error() -> UInt32 {
        let result = BuiltinLs::Phi::print_entry(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc run_all_tests() -> UInt32 {
        let passed = 0;
        
        passed = passed + BuiltinLsTests::test_k_run();
        passed = passed + BuiltinLsTests::test_k_list_entries();
        passed = passed + BuiltinLsTests::test_k_print_entry();
        passed = passed + BuiltinLsTests::test_q_run_returns_error();
        passed = passed + BuiltinLsTests::test_q_list_entries_returns_error();
        passed = passed + BuiltinLsTests::test_q_print_entry_returns_error();
        passed = passed + BuiltinLsTests::test_phi_run_returns_error();
        passed = passed + BuiltinLsTests::test_phi_list_entries_returns_error();
        passed = passed + BuiltinLsTests::test_phi_print_entry_returns_error();
        
        return passed;
    }
}
