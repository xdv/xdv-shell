// Shell Loop Tests

forge ShellLoopTests {
    
    proc test_k_run_loop() -> UInt32 {
        let result = ShellLoop::K::run_loop();
        return 1;
    }
    
    proc test_k_process_line() -> UInt32 {
        let result = ShellLoop::K::process_line(0);
        return 1;
    }
    
    proc test_k_handle_exit() -> UInt32 {
        let result = ShellLoop::K::handle_exit(0);
        return 1;
    }
    
    proc test_k_print_error() -> UInt32 {
        let result = ShellLoop::K::print_error(0);
        return 1;
    }
    
    proc test_q_run_loop_returns_error() -> UInt32 {
        let result = ShellLoop::Q::run_loop();
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_process_line_returns_error() -> UInt32 {
        let result = ShellLoop::Q::process_line(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_handle_exit_returns_error() -> UInt32 {
        let result = ShellLoop::Q::handle_exit(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_print_error_returns_error() -> UInt32 {
        let result = ShellLoop::Q::print_error(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_run_loop_returns_error() -> UInt32 {
        let result = ShellLoop::Phi::run_loop();
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_process_line_returns_error() -> UInt32 {
        let result = ShellLoop::Phi::process_line(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_handle_exit_returns_error() -> UInt32 {
        let result = ShellLoop::Phi::handle_exit(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_print_error_returns_error() -> UInt32 {
        let result = ShellLoop::Phi::print_error(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc run_all_tests() -> UInt32 {
        let passed = 0;
        
        passed = passed + ShellLoopTests::test_k_run_loop();
        passed = passed + ShellLoopTests::test_k_process_line();
        passed = passed + ShellLoopTests::test_k_handle_exit();
        passed = passed + ShellLoopTests::test_k_print_error();
        passed = passed + ShellLoopTests::test_q_run_loop_returns_error();
        passed = passed + ShellLoopTests::test_q_process_line_returns_error();
        passed = passed + ShellLoopTests::test_q_handle_exit_returns_error();
        passed = passed + ShellLoopTests::test_q_print_error_returns_error();
        passed = passed + ShellLoopTests::test_phi_run_loop_returns_error();
        passed = passed + ShellLoopTests::test_phi_process_line_returns_error();
        passed = passed + ShellLoopTests::test_phi_handle_exit_returns_error();
        passed = passed + ShellLoopTests::test_phi_print_error_returns_error();
        
        return passed;
    }
}
