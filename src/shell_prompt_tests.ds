// Shell Prompt Tests

forge ShellPromptTests {
    
    proc test_k_generate() -> UInt32 {
        let result = ShellPrompt::K::generate();
        return 1;
    }
    
    proc test_k_get_cwd() -> UInt32 {
        let result = ShellPrompt::K::get_cwd();
        return 1;
    }
    
    proc test_k_set_cwd() -> UInt32 {
        let result = ShellPrompt::K::set_cwd(0);
        return 1;
    }
    
    proc test_k_get_username() -> UInt32 {
        let result = ShellPrompt::K::get_username();
        return 1;
    }
    
    proc test_k_get_hostname() -> UInt32 {
        let result = ShellPrompt::K::get_hostname();
        return 1;
    }
    
    proc test_k_format_prompt() -> UInt32 {
        let result = ShellPrompt::K::format_prompt();
        return 1;
    }
    
    proc test_q_generate() -> UInt32 {
        let result = ShellPrompt::Q::generate();
        return 1;
    }
    
    proc test_q_get_cwd() -> UInt32 {
        let result = ShellPrompt::Q::get_cwd();
        return 1;
    }
    
    proc test_q_set_cwd_returns_error() -> UInt32 {
        let result = ShellPrompt::Q::set_cwd(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_get_username() -> UInt32 {
        let result = ShellPrompt::Q::get_username();
        return 1;
    }
    
    proc test_q_get_hostname() -> UInt32 {
        let result = ShellPrompt::Q::get_hostname();
        return 1;
    }
    
    proc test_q_format_prompt() -> UInt32 {
        let result = ShellPrompt::Q::format_prompt();
        return 1;
    }
    
    proc test_phi_generate() -> UInt32 {
        let result = ShellPrompt::Phi::generate();
        return 1;
    }
    
    proc test_phi_get_cwd() -> UInt32 {
        let result = ShellPrompt::Phi::get_cwd();
        return 1;
    }
    
    proc test_phi_set_cwd_returns_error() -> UInt32 {
        let result = ShellPrompt::Phi::set_cwd(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_phi_get_username() -> UInt32 {
        let result = ShellPrompt::Phi::get_username();
        return 1;
    }
    
    proc test_phi_get_hostname() -> UInt32 {
        let result = ShellPrompt::Phi::get_hostname();
        return 1;
    }
    
    proc test_phi_format_prompt() -> UInt32 {
        let result = ShellPrompt::Phi::format_prompt();
        return 1;
    }
    
    proc run_all_tests() -> UInt32 {
        let passed = 0;
        
        passed = passed + ShellPromptTests::test_k_generate();
        passed = passed + ShellPromptTests::test_k_get_cwd();
        passed = passed + ShellPromptTests::test_k_set_cwd();
        passed = passed + ShellPromptTests::test_k_get_username();
        passed = passed + ShellPromptTests::test_k_get_hostname();
        passed = passed + ShellPromptTests::test_k_format_prompt();
        passed = passed + ShellPromptTests::test_q_generate();
        passed = passed + ShellPromptTests::test_q_get_cwd();
        passed = passed + ShellPromptTests::test_q_set_cwd_returns_error();
        passed = passed + ShellPromptTests::test_q_get_username();
        passed = passed + ShellPromptTests::test_q_get_hostname();
        passed = passed + ShellPromptTests::test_q_format_prompt();
        passed = passed + ShellPromptTests::test_phi_generate();
        passed = passed + ShellPromptTests::test_phi_get_cwd();
        passed = passed + ShellPromptTests::test_phi_set_cwd_returns_error();
        passed = passed + ShellPromptTests::test_phi_get_username();
        passed = passed + ShellPromptTests::test_phi_get_hostname();
        passed = passed + ShellPromptTests::test_phi_format_prompt();
        
        return passed;
    }
}
