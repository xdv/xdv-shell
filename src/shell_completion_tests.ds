    proc test_phi_add_completion_returns_error() -> UInt32 {
        let result = ShellCompletion::Phi::add_completion(0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc run_all_tests() -> UInt32 {
        let passed = 0;
        
        passed = passed + ShellCompletionTests::test_k_complete();
        passed = passed + ShellCompletionTests::test_k_get_completion_count();
        passed = passed + ShellCompletionTests::test_k_get_completion();
        passed = passed + ShellCompletionTests::test_k_complete_files();
        passed = passed + ShellCompletionTests::test_k_complete_commands();
        passed = passed + ShellCompletionTests::test_k_add_completion();
        passed = passed + ShellCompletionTests::test_q_complete_returns_error();
        passed = passed + ShellCompletionTests::test_q_get_completion_count();
        passed = passed + ShellCompletionTests::test_q_get_completion();
        passed = passed + ShellCompletionTests::test_q_complete_files_returns_error();
        passed = passed + ShellCompletionTests::test_q_complete_commands_returns_error();
        passed = passed + ShellCompletionTests::test_q_add_completion_returns_error();
        passed = passed + ShellCompletionTests::test_phi_complete_returns_error();
        passed = passed + ShellCompletionTests::test_phi_get_completion_count();
        passed = passed + ShellCompletionTests::test_phi_get_completion();
        passed = passed + ShellCompletionTests::test_phi_complete_files_returns_error();
        passed = passed + ShellCompletionTests::test_phi_complete_commands_returns_error();
        passed = passed + ShellCompletionTests::test_phi_add_completion_returns_error();
        
        return passed;
    }
