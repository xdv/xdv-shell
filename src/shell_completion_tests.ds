// Shell Completion Tests

forge ShellCompletionTests {
    
    proc test_k_complete() -> UInt32 {
        let result = ShellCompletion::K::complete(0, 0);
        return 1;
    }
    
    proc test_k_get_completion_count() -> UInt32 {
        let result = ShellCompletion::K::get_completion_count();
        return 1;
    }
    
    proc test_k_get_completion() -> UInt32 {
        let result = ShellCompletion::K::get_completion(0);
        return 1;
    }
    
