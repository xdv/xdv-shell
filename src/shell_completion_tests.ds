        return 1;
    }
    
    proc test_q_complete_returns_error() -> UInt32 {
        let result = ShellCompletion::Q::complete(0, 0);
        if result == 100 {
            return 1;
        }
        return 0;
    }
    
    proc test_q_get_completion_count() -> UInt32 {
